<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.1" />
  </startup>
  <appSettings>
    <!-- If you don't know what to input, please check the Parcel Monkey official CSV spec: https://s3-eu-west-1.amazonaws.com/international-live/files/bulk-shipping-parcel-monkey-csv-spec.pdf -->
    <!-- Package info -->
    <!-- Yep, Etsy doesn't include these info in their exported file -->
    <add key="Weight" value="0.5kg" />
    <add key="Length" value="20cm" />
    <add key="Width" value="15cm" />
    <add key="Height" value="10cm" />

    <!-- Sender's info -->
    <!-- Change all of them to your own info -->
    <add key="SenderName" value="Darth Vader" />
    <add key="SenderEmail" value="lordvader@starwars.com" />
    <add key="SenderPhone" value="123456789" />
    <add key="SenderAddressOrganisation" value="Imperial Forces " />
    <add key="SenderAddressLine1" value="100 Avenue of Death Star" />
    <add key="SenderAddressLine2" value="" />
    <add key="SenderAddressCity" value="Death Star" />
    <add key="SenderAddressProvince" value="Empire" />
    <add key="SenderAddressPostcode" value="12345" />
    <add key="SenderAddressCountryCode" value="FR" />
    <add key="IsSenderAddressResidential" value="Yes" />

    <!-- Customs info -->
    <!-- Possible values for the setting below: - Proforma- Commercial -->
    <add key="CustomsInvoiceType" value="Commercial" />
    <!-- Possible values for the setting below: - Sold- Gift- Sample- Repair- Documents- Intra company transfer- Temporary export -->
    <add key="CustomsExportReason" value="Sold" />
    <!-- Possible values for the setting below: - Permanent- Temporary -->
    <add key="CustomsExportType" value="Permanent" />
    <!-- Code of Country of Manufacture, Leave blank or a valid 2 letter ISO country code -->
    <add key="CodeCountryManufacture" value="FR" />
    <!-- Possible values for the setting below: - Business- Private Individual -->
    <add key="SenderCustomsType" value="Business" />
    <add key="SenderCustomsTaxReference" value="FR12345678901" />
    <add key="SenderCustomsCompanyName" value="Imperial Forces" />
    <!-- Possible values for the setting below: - Business- Private Individual -->
    <add key="RecipientCustomsType" value="Private Individual" />
    <!-- A valid 3-letter ISO currency code -->
    <add key="CurrencyCode" value="EUR" />
    <add key="ProductDescription" value="Lightsabers" />
  </appSettings>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="System.Buffers" publicKeyToken="cc7b13ffcd2ddd51" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-4.0.3.0" newVersion="4.0.3.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>